{
  "expo": {
    "sdkVersion": "54.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera to scan QR codes for adding 2FA services."
      }
    },
    "android": {
      "permissions": [
        "CAMERA",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.acktarius.blockchain2faapp"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow SecureAuth to access your camera to scan QR codes."
        }
      ],
      "./withAndroidStrategiesPlugin",
      "expo-secure-store",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "55b2b55e-df3e-4357-9a2c-c562b160e22e"
      }
    }
  }
}
